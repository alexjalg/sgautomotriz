<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="modalidadVenta" namespace="/modalidadVenta" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*ModalidadVenta" method="{1}" class="actions.ModalidadVentaAction">
            <result name="success" type="tiles">mant-modalidadVenta-index</result>
            <result name="adicionar" type="tiles">mant-modalidadVenta-adicionar</result>
            <result name="grabar" type="tiles">mant-modalidadVenta-grabar</result>
            <result name="actualizar" type="tiles">mant-modalidadVenta-grabar</result>
            <result name="vrfSeleccion" type="tiles">mant-modalidadVenta-grabar</result>
            <result name="eliminar" type="tiles">mant-modalidadVenta-eliminar</result>
        </action>
    </package>
</struts>
