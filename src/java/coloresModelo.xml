<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="coloresMod" namespace="/coloresMod" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*ColorMod" method="{1}" class="actions.ColoresModeloAction">
            <result name="success" type="tiles">mant-coloresMod-index</result>
            <result name="adicionar" type="tiles">mant-coloresMod-adicionar</result>
            <result name="listColores" type="tiles">mant-coloresMod-listcolores</result>
            <result name="grabar" type="tiles">mant-coloresMod-grabar</result>
            <result name="vrfSeleccion" type="tiles">mant-coloresMod-grabar</result>
            <result name="eliminar" type="tiles">mant-coloresMod-eliminar</result>
        </action>
    </package>
</struts>
