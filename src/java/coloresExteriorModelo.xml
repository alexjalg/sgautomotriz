<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="coloresExtModelo" namespace="/coloresExtModelo" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*ColorExtModelo" method="{1}" class="actions.ColoresExteriorModeloAction">
            <result name="success" type="tiles">mant-coloresExtModelo-index</result>
            <result name="adicionar" type="tiles">mant-coloresExtModelo-adicionar</result>
            <result name="listColoresExterior" type="tiles">mant-coloresExtModelo-listColoresExt</result>
            <result name="grabar" type="tiles">mant-coloresExtModelo-grabar</result>
            <result name="vrfSeleccion" type="tiles">mant-coloresExtModelo-grabar</result>
            <result name="eliminar" type="tiles">mant-coloresExtModelo-eliminar</result>
        </action>
    </package>
</struts>
