<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="usuarios" namespace="/usuarios" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*Usuario" method="{1}" class="actions.UsuariosAction">
            <result name="success" type="tiles">mant-usuarios-index</result>
            <result name="adicionar" type="tiles">mant-usuarios-adicionar</result>
            <result name="grabar" type="tiles">mant-usuarios-grabar</result>
            <result name="actualizar" type="tiles">mant-usuarios-grabar</result>
            <result name="vrfSeleccion" type="tiles">mant-usuarios-grabar</result>
            <result name="eliminar" type="tiles">mant-usuarios-eliminar</result>
            <result name="actEstado" type="tiles">mant-usuarios-grabar</result>
            <result name="getLocales">/mantenimientos/usuarios/getLocales.jsp</result>
            <result name="updPassword" type="tiles">mant-usuarios-updPass</result>
            <result name="updVarSesionCaducCla" type="tiles">mant-usuarios-grabar</result>
            
            <result name="imprimir">/mantenimientos/usuarios/imprimir.jsp</result>
        </action>
    </package>
</struts>
