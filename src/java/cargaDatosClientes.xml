<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="cargaDatosClientes" namespace="/cargaDatosClientes" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*CargaDatosClientes" method="{1}" class="actions.CargaDatosClientesAction">
            <result name="success" type="tiles">imp-cargaDatosClientes-index</result>
            <result name="grabar">/importaciones/cargaDatosClientes/grabar.jsp</result>
            <result name="verificar" type="tiles">imp-cargaDatosClientes-verificar</result>            
        </action>
    </package>
</struts>
