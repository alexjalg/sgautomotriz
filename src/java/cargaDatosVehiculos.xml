<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="cargaDatosVehiculos" namespace="/cargaDatosVehiculos" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*CargaDatosVehiculos" method="{1}" class="actions.CargaDatosVehiculosAction">
            <result name="success" type="tiles">imp-cargaDatosVehiculos-index</result>
            <result name="grabar" type="tiles">imp-cargaDatosVehiculos-index</result>
            <result name="verificar" type="tiles">imp-cargaDatosVehiculos-verificar</result>            
        </action>
    </package>
</struts>
