<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="tipoDocumentoVenta" namespace="/tipoDocumentoVenta" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*TipoDocumentoVenta" method="{1}" class="actions.TipoDocumentoVentaAction">
            <result name="success" type="tiles">mant-tipoDocumentoVenta-index</result>
            <result name="adicionar" type="tiles">mant-tipoDocumentoVenta-adicionar</result>
            <result name="modificar" type="tiles">mant-tipoDocumentoVenta-adicionar</result>
            <result name="grabar" type="tiles">mant-tipoDocumentoVenta-grabar</result>
            <result name="actualizar" type="tiles">mant-tipoDocumentoVenta-grabar</result>
            <result name="vrfSeleccion">/mantenimientos/tipoDocumentoVenta/grabar.jsp</result>
            <result name="eliminar" type="tiles">mant-tipoDocumentoVenta-eliminar</result>
        </action>
    </package>
</struts>
