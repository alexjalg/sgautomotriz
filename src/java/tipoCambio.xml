<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="tipoCambio" namespace="/tipoCambio" extends="struts-default">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>

        <action name="*TipoCambio" method="{1}" class="actions.TipoCambioAction">
            <result name="success" type="tiles">mant-tipoCambio-index</result>
            <result name="adicionar" type="tiles">mant-tipoCambio-adicionar</result>
            <result name="grabar">/mantenimientos/tipoCambio/grabar.jsp</result>
            <result name="actualizar">/mantenimientos/tipoCambio/grabar.jsp</result>
            <result name="vrfSeleccion">/mantenimientos/tipoCambio/grabar.jsp</result>
            <result name="eliminar">/mantenimientos/tipoCambio/eliminar.jsp</result>
        </action>
    </package>
</struts>
